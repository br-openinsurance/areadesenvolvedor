        </span><span class="nl">"description"</span><span class="p">:</span><span class="w"> </span><span class="s2">"BRL"</span><span class="w">
      </span><span class="p">}</span><span class="w">
    </span><span class="p">},</span><span class="w">
    </span><span class="nl">"maxValue"</span><span class="p">:</span><span class="w"> </span><span class="p">{</span><span class="w">
      </span><span class="nl">"amount"</span><span class="p">:</span><span class="w"> </span><span class="s2">"2000.00"</span><span class="p">,</span><span class="w">
      </span><span class="nl">"unit"</span><span class="p">:</span><span class="w"> </span><span class="p">{</span><span class="w">
        </span><span class="nl">"code"</span><span class="p">:</span><span class="w"> </span><span class="s2">"R$"</span><span class="p">,</span><span class="w">
        </span><span class="nl">"description"</span><span class="p">:</span><span class="w"> </span><span class="s2">"BRL"</span><span class="w">
      </span><span class="p">}</span><span class="w">
    </span><span class="p">},</span><span class="w">
    </span><span class="nl">"percentage"</span><span class="p">:</span><span class="w"> </span><span class="s2">"10.00"</span><span class="w">
  </span><span class="p">},</span><span class="w">
  </span><span class="nl">"gracePeriod"</span><span class="p">:</span><span class="w"> </span><span class="mi">0</span><span class="p">,</span><span class="w">
  </span><span class="nl">"gracePeriodicity"</span><span class="p">:</span><span class="w"> </span><span class="s2">"DIA"</span><span class="p">,</span><span class="w">
