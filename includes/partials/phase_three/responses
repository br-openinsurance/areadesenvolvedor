      </span><span class="nl">"description"</span><span class="p">:</span><span class="w"> </span><span class="s2">"BRL"</span><span class="w">
    </span><span class="p">}</span><span class="w">
  </span><span class="p">},</span><span class="w">
  </span><span class="nl">"paymentType"</span><span class="p">:</span><span class="w"> </span><span class="s2">"BOLETO"</span><span class="w">
</span><span class="p">}</span><span class="w">

