  </span><span class="nl">"data"</span><span class="p">:</span><span class="w"> </span><span class="p">{</span><span class="w">
    </span><span class="nl">"consentId"</span><span class="p">:</span><span class="w"> </span><span class="s2">"urn:initiator:C1DD93123"</span><span class="p">,</span><span class="w">
