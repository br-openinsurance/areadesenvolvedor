  </span><span class="nl">&quot;consentId&quot;</span><span class="p">:</span><span class="w"> </span><span class="s2">&quot;urn:bancoex:C1DD33123&quot;</span><span class="p">,</span><span class="w">
  </span><span class="nl">&quot;step&quot;</span><span class="p">:</span><span class="w"> </span><span class="s2">&quot;consent-created&quot;</span><span class="p">,</span><span class="w">
