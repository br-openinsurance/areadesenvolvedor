<h2 id="tocS_successmetricas">success</h2>

<!-- backwards compatibility -->

<p><a id="schemasuccessmetricas"></a>
<a id="schema_success"></a>
<a id="tocSsuccess"></a>
<a id="tocssuccess"></a></p>

<p><div class="highlight"><pre class="highlight json tab-json"><code><span class="p">{</span><span class="w">
  </span><span class="nl">&quot;reportId&quot;</span><span class="p">:</span><span class="w"> </span><span class="s2">&quot;7eb81ba1-9011-4c58-9d5b-1098550d2fca&quot;</span><span class="p">,</span><span class="w">
  </span><span class="nl">&quot;correlationId&quot;</span><span class="p">:</span><span class="w"> </span><span class="s2">&quot;FWUCp4yOA1b87LUJpgn1Mf52wi9KEF11&quot;</span><span class="p">,</span><span class="w">
  </span><span class="nl">&quot;status&quot;</span><span class="p">:</span><span class="w"> </span><span class="s2">&quot;ACCEPTED&quot;</span><span class="w">
