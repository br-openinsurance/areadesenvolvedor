<h2 id="tocS_InsurancePetroleumCoverage">InsurancePetroleumCoverage</h2>

<!-- backwards compatibility -->

<p><a id="schemainsurancepetroleumcoverage"></a>
<a id="schema_InsurancePetroleumCoverage"></a>
<a id="tocSinsurancepetroleumcoverage"></a>
<a id="tocsinsurancepetroleumcoverage"></a></p>
<div class="highlight"><pre class="highlight json tab-json"><code><span class="p">{</span><span class="w">
  </span><span class="nl">"branch"</span><span class="p">:</span><span class="w"> </span><span class="mi">111</span><span class="p">,</span><span class="w">
  </span><span class="nl">"code"</span><span class="p">:</span><span class="w"> </span><span class="s2">"RISCOS_DE_PETROLEO"</span><span class="p">,</span><span class="w">
  </span><span class="nl">"description"</span><span class="p">:</span><span class="w"> </span><span class="s2">"string"</span><span class="p">,</span><span class="w">
