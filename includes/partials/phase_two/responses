    </span><span class="nl">"transactionToDateTime"</span><span class="p">:</span><span class="w"> </span><span class="s2">"2021-02-01T23:59:59Z"</span><span class="p">,</span><span class="w">
    </span><span class="nl">"rejection"</span><span class="p">:</span><span class="w"> </span><span class="p">{</span><span class="w">
      </span><span class="nl">"rejectedBy"</span><span class="p">:</span><span class="w"> </span><span class="s2">"USER"</span><span class="p">,</span><span class="w">
      </span><span class="nl">"reason"</span><span class="p">:</span><span class="w"> </span><span class="p">{</span><span class="w">
        </span><span class="nl">"code"</span><span class="p">:</span><span class="w"> </span><span class="s2">"CONSENT_EXPIRED"</span><span class="p">,</span><span class="w">
        </span><span class="nl">"additionalInformation"</span><span class="p">:</span><span class="w"> </span><span class="s2">"Tempo de confirmação da múltipla alçada excedido."</span><span class="w">
      </span><span class="p">}</span><span class="w">
    </span><span class="p">}</span><span class="w">
